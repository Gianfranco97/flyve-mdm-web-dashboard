{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.DS_Store": true,
    "**/.gtm": true,
    "**/.sass-cache": true,
    "**/.tmp": true,
    "**/node_modules": true,
    "**/bower_components": true
  },
  "files.associations": {
    ".htmlhintrc": "json"
  },
  "files.encoding": "utf8",
  "files.eol": "\r\n",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/.vscode": true
  },
  "editor.formatOnSave": true,
  "jshint.enable": true,
  "jshint.config": null,
  "jshint.options": {},
  "jshint.excludePath": null,
  "jshint.exclude": {},
  "beautify.onSave": true,
  "sasslint.enable": true,
  "sasslint.configFile": ".sass-lint.yml"
  //"sasslint.resolvePathsRelativeToConfig":
}